{
    "name": "@bubblehunt/logger",
    "version": "1.3.2",
    "description": "Helper for writing logs using transports",
    "author": "krot47 <lelekodmitry@gmail.com>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KROT47/logger.git"
    },
    "main": "dist/index",
    "engines": {
        "node": "*"
    },
    "scripts": {
        "clean:dist": "rm -rf dist/*",
        "build": "$npm_execpath run clean:dist && node_modules/.bin/babel-node build",
        "test": "yarn build && node_modules/.bin/babel-node tests/test",
        "update-snapshots": "node_modules/.bin/babel-node tests/update-tests-snapshots",
        "flow": "node_modules/.bin/flow"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-flow": "^6.23.0",
        "flow-bin": "0.69.0",
        "fs-extra": "^7.0.1",
        "klaw-sync": "^3.0.2",
        "lodash": "^4.17.5"
    },
    "license": "MIT",
    "dependencies": {
        "get-explicit-type": "^1.0.2",
        "mkdirp": "^0.5.1"
    }
